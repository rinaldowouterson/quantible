{
  "name": "quantible",
  "description": "A versatile library for extracting and converting numerical values, units, currency, and mathematical expressions into their spoken word equivalents.",
  "version": "0.2.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.iife.min.js",
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && tsup",
    "test": "jest _tests_",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "number",
    "unit",
    "converter",
    "natural language",
    "spoken word",
    "quantity",
    "expression",
    "spoken words",
    "currency",
    "math",
    "text-to-speech",
    "tts",
    "numerical values"
  ],
  "author": "rinaldowouterson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rinaldowouterson/quantible.git"
  },
  "homepage": "https://github.com/rinaldowouterson/quantible#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.2"
  }
}